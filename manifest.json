{
    "name": "Track the Trackers",
    "description": "Monitor the trackers used in the website ",
    "version": "1.0",
    "manifest_version": 3,
    "background": {
        "service_worker": "background.js"
      },
    "permissions": ["storage", "activeTab", "scripting","unlimitedStorage"],
    "action": {
        "default_popup": "popup.html"
      }
  }